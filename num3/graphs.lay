#!MC 1410
$!VarSet |LFDSFN1| = '"U_plus_two-layer_mixing_model.dat"'
$!VarSet |LFDSVL1| = '"'y+'" "'U+'" "'y+/R+'" "'U+/U+max'" "'normalized_Reynolds_Shear_Stress'"'
